meta {
  name: login
  type: http
  seq: 2
}

post {
  url: {{HOST}}auth/jwt/create/
  body: multipartForm
  auth: apikey
}

auth:apikey {
  key: JWT
  value: {{TOKEN}}
  placement: header
}

body:multipart-form {
  username: test
  password: test
}

vars:post-response {
  TOKEN: res.body.access
}

assert {
  res.status: eq 200
}

settings {
  encodeUrl: true
  timeout: 0
}
