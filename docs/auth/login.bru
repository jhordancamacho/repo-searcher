meta {
  name: login
  type: http
  seq: 2
}

post {
  url: {{HOST}}auth/token/login/
  body: multipartForm
  auth: inherit
}

body:multipart-form {
  username: test
  password: test
}

vars:post-response {
  TOKEN: res.body.auth_token
}

assert {
  res.status: eq 200
}

settings {
  encodeUrl: true
  timeout: 0
}
